void fxn(TreeNode * root , vector<int> &ans)
{
    if(root==NULL) return ;
    fxn(root->left,ans);
    
    fxn(root->right,ans);
    ans.push_back(root->data);

}
vector<int> getPostOrderTraversal(TreeNode *root)
{
    // Write your code here.
     vector<int> ans;
    fxn(root,ans);
    return ans;
}
