string longestCommonPrefix(vector<string> &arr, int n)
{
    // Write your code here
      string ans = arr[0];

    for(int i=1; i<n; i++) {

        string temp = "";

        int mini = min(ans.size(), arr[i].size());

        for(int j=0; j<mini; j++) {

            if(arr[i][j] == ans[j]) temp += ans[j];

            else break;

        }

        ans = temp;

    }

    return ans;
}


