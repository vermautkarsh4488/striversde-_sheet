
vector<int> getLevelOrder(BinaryTreeNode<int> *root)
{
    //  Write your code here.
 vector<int> ans;
    if(root==NULL) return ans;
    queue<BinaryTreeNode<int>*> q;
    q.push(root);
   

    while(!q.empty())
    {
        int s = q.size();
        for(int i = 0; i<s; i++)
        {
            BinaryTreeNode<int> *temp = q.front();
            q.pop();

            if(temp->left)
            {
                q.push(temp->left);

            }
            if(temp->right)
            {
                q.push(temp->right);
            }

            ans.push_back(temp->val);

        }
    }

    return ans;

}
